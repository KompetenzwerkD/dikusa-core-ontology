# allgemeine value properties

:has_main_value
    rdf:type rdf:Property ;
    rdfs:label "has main value" ;    
    rdfs:subPropertyOf rdf:value ;
    rdfs:subPropertyOf :has_value .

:has_value
    rdf:type rdf:Property ;
    rdfs:range rdfs:Literal ;
    rdfs:label "has value" .

# Properties for provenance or comments for making statements about triples

:has_provenence_text
    rdf:type rdf:Property ;
    rdfs:label "has provenance text" ;
    rdfs:subPropertyOf :has_triple_value .

:has_provenence_text_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_provenence_text ;
    sh:nodeKind sh:Literal .

:has_provenence_URI
    rdf:type rdf:Property ;
    rdfs:label "has provenance URI" ;
    rdfs:subPropertyOf rdfs:seeAlso ;
    rdfs:subPropertyOf :has_triple_value .

:has_provenence_URI_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_provenence_URI ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:anyURI ;
    sh:pattern "^http" .

:has_provenance_certainty
    rdf:type rdf:Property ;
    rdfs:label "has provenance certainty" ;
    rdfs:subPropertyOf :has_triple_value .

:has_provenance_certainty_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_provenance_certainty ;
    sh:nodeKind sh:Literal .

:has_comment
    rdf:type rdf:Property ;
    rdfs:label "has comment" ;
    rdfs:subPropertyOf  rdfs:comment ;
    rdfs:subPropertyOf :has_triple_value .

:has_comment_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_comment ;
    sh:nodeKind sh:Literal .

:has_description
    rdf:type rdf:Property ;
    rdfs:label "has description" ;
    rdfs:subPropertyOf :has_triple_value .

:has_description_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_description ;
    sh:nodeKind sh:Literal .

:has_triple_value
    rdf:type rdf:Property ;
    rdfs:domain rdf-star:Triple ;
    rdfs:range rdfs:Literal ;
    rdfs:label "has triple value" ;
    rdfs:subPropertyOf :has_value .

