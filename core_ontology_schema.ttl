# Properties for making provenance statements about triples
# the following statements describe the action of entering it into the knowledge graph

# which group (institution or project) contributed this fact to the knowledge base
:has_provenence_contributor
    rdf:type rdf:Property ;
    rdfs:label "has provenance contributor" ;
    rdfs:domain rdf-star:Triple ;
    rdfs:range :group .

:has_provenence_contributor_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_provenence_contributor ;
    sh:nodeKind sh:IRI ;
    sh:class :group .

# the date the fact was entered into the graph
:has_provenence_date
    rdf:type rdf:Property ;
    rdfs:label "has provenance date" ;
    rdfs:domain rdf-star:Triple ;
    rdfs:range xsd:date .

:has_provenence_date_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_provenence_date ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:date ;
    sh:pattern "^[0-9][0-9][0-9][0-9]-[0-9][0-9]-[0-9][0-9]" .

# general property for statements on triples
:has_triple_value
    rdf:type rdf:Property ;
    rdfs:domain rdf-star:Triple ;
    rdfs:range rdfs:Literal ;
    rdfs:label "has triple value" ;
    rdfs:subPropertyOf :has_value .

