    rdf:type rdf:Property ;
    rdfs:comment "A relation between a citation and the resource where this citation is to be found." ;
    rdfs:label "has citation resource"@en ;
    rdfs:label "hat Zitationsressource"@de ;
    rdfs:domain :citation ;
    rdfs:range :work ;
    rdfs:subPropertyOf rdf:value .

