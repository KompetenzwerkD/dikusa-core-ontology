    rdfs:domain :timepoint_related_entity ;
    rdfs:range xsd:date ;
    rdfs:subPropertyOf :has_value .

:has_begin_earliest
    rdf:type rdf:Property ;
    rdfs:label "has begin earliest" ;
    rdfs:domain :timeframe_related_entity ;
    rdfs:range xsd:date ;
    rdfs:subPropertyOf :has_value .

:has_begin_latest
    rdf:type rdf:Property ;
    rdfs:label "has begin latest" ;
    rdfs:domain :timeframe_related_entity ;
    rdfs:range xsd:date ;
    rdfs:subPropertyOf :has_value .
    
:has_end_earliest
    rdf:type rdf:Property ;
    rdfs:label "has end earliest" ;
    rdfs:domain :timeframe_related_entity ;
    rdfs:range xsd:date ;
    rdfs:subPropertyOf :has_value .

:has_end_latest
    rdf:type rdf:Property ;
    rdfs:label "has end latest" ;
    rdfs:domain :timeframe_related_entity ;
    rdfs:range xsd:date ;
    rdfs:subPropertyOf :has_value .

