:life_state_change
    rdf:type rdfs:Class , sh:NodeShape ;
    rdfs:label "life state change" ;
    rdfs:subClassOf :entity_with_timepoint ;
    rdfs:subClassOf :entity_with_name ;
    rdfs:subClassOf :entity_with_description ;
    rdfs:subClassOf :entity_with_place ;
    sh:or (
        [
        sh:path [ sh:inversePath :has_death ] ;
            sh:minCount 1 ;
        ]
        [
        sh:path [ sh:inversePath :has_birth ] ;
            sh:minCount 1 ;
        ]
    ) .

