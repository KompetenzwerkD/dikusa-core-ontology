:change_of_existence
    rdf:type rdfs:Class ;
    rdfs:comment "Superclass for the events marked as the beginning end or change of the existence of an entity, subclass of event" ;
    rdfs:subClassOf :event ;
    rdfs:label "change of existence"@en ;
    rdfs:label "Änderung der Existenz"@de ;
    sh:or (
        [
        sh:path [ sh:inversePath :has_end_of_existence ] ;
            sh:minCount 1 ;
        ]
        [
        sh:path [ sh:inversePath :has_begin_of_existence ] ;
            sh:minCount 1 ;
        ]
        [
        sh:path [ sh:inversePath :has_change_of_existence ] ;
            sh:minCount 1 ;
        ]
    ) .

