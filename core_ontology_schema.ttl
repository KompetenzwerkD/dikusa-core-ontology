# --------------------------

:has_name
    rdf:type rdf:Property ;
    rdfs:label "has name" ;
    rdfs:domain :entity_with_name ;
    rdfs:range :name .

:has_name_i
    rdf:type rdf:Property ;
    owl:inverseOf :has_name .

:has_preferred_name
    rdf:type rdf:Property ;
    rdfs:label "has preferred name" ;
    rdfs:subPropertyOf :has_name .

:has_preferred_name_i
    rdf:type rdf:Property ;
    owl:inverseOf :has_preferred_name .

:has_alternative_name
    rdf:type rdf:Property ;
    rdfs:label "has alternative name" ;
    rdfs:subPropertyOf :has_name .

:has_alternative_name_i
    rdf:type rdf:Property ;
    owl:inverseOf :has_alternative_name .

:has_preferred_name_subject_shape
	a sh:NodeShape ;
	sh:targetSubjectsOf :has_preferred_name ;
    sh:nodeKind sh:IRI ;
    sh:class :entity_with_name .

:has_preferred_name_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_preferred_name ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:class :name .

:has_alternative_name_subject_shape
	a sh:NodeShape ;
	sh:targetSubjectsOf :has_alternative_name ;
    sh:nodeKind sh:IRI ;
    sh:class :entity_with_name .

:has_alternative_name_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_alternative_name ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:class :name .

:name
    rdf:type rdfs:Class, sh:NodeShape  ;
    rdfs:subClassOf :timeframe_related_entity ;
    rdfs:label "name" ;
    sh:property [
        sh:path :has_name_value ;
        sh:minCount 1 ;
        sh:maxCount 1
    ] ;
    sh:property [
        sh:path :has_script ;
        sh:maxCount 1
    ] .

:has_name_value
    rdf:type rdf:Property ;
    rdfs:label "has name value" ;
    rdfs:domain :name ;
    rdfs:subPropertyOf :has_main_value .

:has_name_value_subject_shape
	a sh:NodeShape ;
	sh:targetSubjectsOf :has_name_value ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:class :name .

:has_name_value_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_name_value ;
    sh:nodeKind sh:Literal .

:has_script
    rdf:type rdf:Property ;
    rdfs:label "has script" ;
    rdfs:domain :name ;
    rdfs:range :script .

:has_script_i
    rdf:type rdf:Property ;
    owl:inverseOf :has_script .

:has_script_subject_shape
	a sh:NodeShape ;
	sh:targetSubjectsOf :has_script ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:class :name .

:has_script_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_script ;
    sh:nodeKind sh:IRI ;
    sh:class :script .

:script
    rdf:type rdfs:Class ;
    rdfs:subClassOf :entity_with_name ;
    rdfs:subClassOf :entity_with_description .

