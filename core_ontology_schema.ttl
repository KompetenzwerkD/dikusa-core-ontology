prefix : <http://www.werk.saw-leipzig.de/>
prefix werkd: <http://www.werk.saw-leipzig.de/>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
prefix rdf-star:  <http://www.w3.org/ns/rdf-star#>
prefix owl:	<http://www.w3.org/2002/07/owl#>

:person
    rdf:type rdfs:Class ;
    rdfs:subClassOf :entity_with_name ;
    rdfs:subClassOf :entity_with_description ;
    rdfs:subClassOf :entity_with_citation ;
    rdfs:subClassOf :agent ;
    rdfs:label "Person"@en.

# -----------------------

:has_name
    rdf:type rdf:Property ;
    rdfs:label "has name" ;
    rdfs:domain :entity_with_name ;
    rdfs:range :name .

:has_preferred_name
    rdf:type rdf:Property ;
    rdfs:label "has preferred name" ;
    rdfs:subPropertyOf :has_name .

:has_alternative_name
    rdf:type rdf:Property ;
    rdfs:label "has alternative name" ;
    rdfs:subPropertyOf :has_name .

:name
    rdf:type rdfs:Class ;
    rdfs:subClassOf :timeframe_related_entity ;
    rdfs:label "name" .

:has_name_value
    rdf:type rdf:Property ;
    rdfs:label "has name value" ;
    rdfs:domain :name ;
    rdfs:subPropertyOf :has_main_value .

:has_script
    rdf:type rdf:Property ;
    rdfs:label "has script" ;
    rdfs:domain :name ;
    rdfs:range :script .

# ------------------------------

# allgemeine value properties

:has_main_value
    rdf:type rdf:Property ;
    rdfs:label "has main value" ;    
    rdfs:subPropertyOf rdf:value ;
    rdfs:subPropertyOf :has_value .

:has_value
    rdf:type rdf:Property ;
    rdfs:range rdfs:Literal ;
    rdfs:label "has value" .

    
# Properties for provenance or comments for making statements about triples

:has_provenence_text
    rdf:type rdf:Property ;
    rdfs:label "has provenance text" ;
    rdfs:subPropertyOf :has_triple_value .

:has_provenence_URI
    rdf:type rdf:Property ;
    rdfs:label "has provenance URI" ;
    rdfs:subPropertyOf rdfs:seeAlso ;
    rdfs:subPropertyOf :has_triple_value .

:has_provenance_certainty
    rdf:type rdf:Property ;
    rdfs:label "has provenance certainty" ;
    rdfs:subPropertyOf :has_triple_value .

:has_comment
    rdf:type rdf:Property ;
    rdfs:label "has comment" ;
    rdfs:subPropertyOf  rdfs:comment ;
    rdfs:subPropertyOf :has_triple_value .

:has_description
    rdf:type rdf:Property ;
    rdfs:label "has description" ;
    rdfs:subPropertyOf :has_triple_value .

:has_triple_value
    rdf:type rdf:Property ;
    rdfs:domain rdf-star:Triple ;
    rdfs:range rdfs:Literal ;
    rdfs:label "has triple value" ;
    rdfs:subPropertyOf :has_value .

# ----------------------
    
:has_entity_description
    rdf:type rdf:Property ;
    rdfs:label "has description" ;
    rdfs:domain :entity_with_description ;
    rdfs:range :description .

:entity_description rdf:type rdfs:Class ;
    rdfs:label "description" .

:has_entity_descripton_value
    rdf:type rdf:Property ;
    rdfs:label "has entity description value" ;
    rdfs:domain :description ;
    rdfs:subPropertyOf :has_main_value .

:has_entity_description_author
    rdf:type rdf:Property ;
    rdfs:label "has entity description author" ;
    rdfs:domain :description ;
    rdfs:range :person .

# ----------------------

:has_gender
    rdf:type rdf:Property ;
    rdfs:label "has gender" ;
    rdfs:domain :person ;
    rdfs:range :gender .

