# Properties for making provenance statements about triples
# the following statements describe the origin of the fact not the action of entering it into the knowledge graph

:has_provenence_text
    rdf:type rdf:Property ;
    rdfs:label "has provenance text" ;
    rdfs:subPropertyOf :has_triple_value .

:has_provenence_text_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_provenence_text ;
    sh:nodeKind sh:Literal .

:has_provenence_resource
    rdf:type rdf:Property ;
    rdfs:label "has provenance resource" ;
    rdfs:domain rdf-star:Triple .

:has_provenence_resource_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_provenence_resource ;
    sh:nodeKind sh:IRI .

:has_provenence_URL
    rdf:type rdf:Property ;
    rdfs:label "has provenance URL" ;
    rdfs:subPropertyOf rdfs:seeAlso ;
    rdfs:subPropertyOf :has_triple_value .

:has_provenence_URL_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_provenence_URL ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:anyURI ;
    sh:pattern "^http" .

:has_provenance_certainty
    rdf:type rdf:Property ;
    rdfs:label "has provenance certainty" ;
    rdfs:subPropertyOf :has_triple_value .

:has_provenance_certainty_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_provenance_certainty ;
    sh:nodeKind sh:Literal .

# general comments or descriptions for describing triples

:has_comment
    rdf:type rdf:Property ;
    rdfs:label "has comment" ;
    rdfs:subPropertyOf  rdfs:comment ;
    rdfs:subPropertyOf :has_triple_value .

:has_comment_object_shape
	a sh:NodeShape ;
