:has_death
    rdf:type rdf:Property ;
    rdfs:label "has death" ;
    rdfs:subPropertyOf :has_life_state_change .

:has_death_subject_shape
	a sh:NodeShape ;
	sh:targetSubjectsOf :has_death ;
    sh:nodeKind sh:IRI ;
    sh:class :person .

:has_death_object_shape
	a sh:NodeShape ;
    sh:targetObjectsOf :has_death ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:class :life_state_change .

:has_death_i
    rdf:type rdf:Property ;
