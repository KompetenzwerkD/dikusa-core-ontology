    rdfs:subClassOf :entity_with_place_relation ;
    rdfs:subClassOf :entity_with_group_relation ;
    rdfs:subClassOf :entity_with_person_relation ;    
    rdfs:label "Person"@en ;
    sh:property [
        sh:path :has_preferred_name ;
        sh:minCount 1
    ] .

:place
